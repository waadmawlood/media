{
    "name": "waad/media",
    "description": "Add media to your Laravel application in one place",
    "keywords": [
        "Waad Mawlood",
        "Media",
        "laravel-media",
        "files",
        "media-php",
        "upload file"
    ],
    "homepage": "https://github.com/waadmawlood/media",
    "license": "MIT",
    "authors": [
        {
            "name": "Waad Mawlood",
            "email": "waad_mawlood@outlook.com",
            "homepage": "https://waad.netlify.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.12.0"
    },
    "autoload": {
        "psr-4": {
            "Waad\\Media\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Waad\\Media\\MediaServiceProvider"
            ]
        }
    }
}
